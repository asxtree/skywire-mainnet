[Unit]
Description=Skywire Manager node start/stop service
After=network.target
After=network-online.target

[Service]
Type=oneshot
ExecStart=/usr/local/skywire/go/src/github.com/asxtree/skywire-mainnet/systemd-scripts/manager_start
ExecStop=/usr/local/skywire/go/src/github.com/asxtree/skywire-mainnet/systemd-scripts/stop
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target